-- github issue #232: missing asterisk for derived attribute / data lost

-- #1=AB(.BEE.);

SCHEMA multiple_inheritance_derived;

TYPE b_name = ENUMERATION OF (B, bee);
END_TYPE;

ENTITY A;
  DERIVE
    name : STRING := 'entity a';
END_ENTITY;

ENTITY B;
    name: b_name;
END_ENTITY;

ENTITY AB
  SUBTYPE OF (A, B);
--   WHERE
--     WR1: SELF\B.name := b_name.bee;
END_ENTITY;

END_SCHEMA;